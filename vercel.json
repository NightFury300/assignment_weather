{
    "version": 2,
    "builds": [
      {
        "src": "src/vercelEntry.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/users/(.*)",
        "dest": "src/vercelEntry.js"
      },
      {
        "src": "^/$",
        "dest": "/api/v1/users/weather"
      },
      {
        "src": "/(.*)", 
        "dest": "/api/v1/users/weather"
      }
    ],
    "crons": [
    {
      "path": "/api/v1/users/cron",
      "schedule": "00 08 * * *"
    }
  ]
  }
  