{
    "version": 2,
    "builds": [
      {
        "src": "src/vercelEntry.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/users/(.*)",
        "dest": "src/vercelEntry.js"
      },
      {
        "src": "^/$",
        "dest": "/api/v1/users/weather"
      }
    ],
    "rewrites": [
    { "source": "/(.*)", "destination": "/api/v1/users/weather" }
    ]
  }
  