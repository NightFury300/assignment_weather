{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "^/$",
            "dest": "/api/v1/users/weather"
          },
          {
            "src": "/api/v1/users/(.*)",
            "dest": "src/index.js"
          }
    ]
  }
  